@model IEnumerable<DellChallenge.D2.Web.Models.ProductModel>

@{
    ViewData["Title"] = "View";
}
<h4>All Products</h4>
<hr />

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-md-4">
            @item.Name - @item.Category
        </div>
        <div>
            <a class="btn btn-primary btnDelete" href="@Url.Action("Update", "Product", new { id = item.Id})">Update</a>
            <a class="btn btn-danger btnDelete" deleteurl="@Url.Action("Delete", "Product", new { id = item.Id})">Delete</a>
        </div>
    </div>
}




